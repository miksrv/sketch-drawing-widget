{
    "opts": {
        "destination": "./jsdoc/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["."],
        "excludePattern": "node_modules",
        "includePattern": ".(jsx|js|ts|tsx)$"
    },
    "plugins": [
        "jsdoc-plugin-typescript"
    ],
    "typescript": {
        "moduleRoot": "src"
    },
    "sourceType": "module"
}
